//函数:统一时间函数
//参数:格式化语句,时间值
//now(),返回+8的Unix时间戳
//now('Y-m-d H:i:s'),返回+8且格式化后的现在的时间
//now('Y-m-d H:i:s',$Rs['addtime']),返回+8且格式化后的指定的时间
function now($x='',$y=0) {
	if(!Is_numeric($y) Or $y<1){$y=time();}
	$y=$y+YJ_SysLocal*3600;
	if($x!='') {
		$Tim=Gmdate(''.$x.'',$y);
	}else{
		$Tim=&$y;
	}
	return $Tim;//StrtoTime($Tim);
}

